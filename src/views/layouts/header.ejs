<!--=====================================
HEADER
======================================-->
<header class="container-fluid"> 
    <div class="container">
        <div class="row" id="cabezote">
            <!--=====================================
            LOGOTIPO
            ======================================-->
            <div class="col-lg-3 col-md-3 col-sm-2 col-xs-12" id="logotipo">
                <a href="/">
                    <img src="/img/myTurista.png" class="img-responsive">
                </a>
            </div>
            <!--=====================================
            BLOQUE CATEGORÍAS Y BUSCADOR
            ======================================-->
            <div class="col-lg-6 col-md-6 col-sm-8 col-xs-12">
                <!--=====================================
                BUSCADOR
                ======================================-->
                <div class="input-group col-lg-12 col-md-12 col-sm-12 col-xs-12" id="buscador">
                    <input type="search" name="buscar" class="form-control" placeholder="Buscar...">
                    <span class="input-group-btn">
                        <a href="#">
                            <button class="btn btn-default backColor" type="submit">
                                <i class="fa fa-search"></i>
                            </button>
                        </a>
                    </span>
                </div>
            </div>
            <!--=====================================
            NOTIFICACION
            ======================================-->
            <div class="col-lg-3 col-md-3 col-sm-2 col-xs-12" id="carrito">
                <a href="/solicitud" class="actualizarNotificaciones">
                    <button class="btn btn-default pull-left backColor">
                        <i class="fa fa-bell" aria-hidden="true"></i>
                    </button>
                </a>
                <p>Notificacion<span class="cantidadCesta"></span> <br> cantidad <span class="sumaCesta"> 0 </span></p>
            </div>
        </div>
    </div>
</header>

